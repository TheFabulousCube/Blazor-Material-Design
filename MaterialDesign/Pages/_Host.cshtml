@page "/"
@namespace MaterialDesign.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
@using EmbeddedBlazorContent
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Material Design</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />
    @Html.EmbeddedBlazorContent()
</head>
<body>
    <app>
        @* Remove the following line of code to disable prerendering *@
        @(await Html.RenderStaticComponentAsync<App>())
    </app>

    @*<script src="https://u5xzz.app.goo.gl/qZs9"></script>*@

    <script src="_framework/blazor.server.js"></script>
    <script src="~/scripts/angular-counter.js"></script>
    <script src="~/scripts/exampleJsInterop.js"></script>

</body>
</html>
